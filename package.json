{
  "name": "simply-result",
  "version": "8.1.1",
  "description": "Simply typesafe Result and Option monads in typescript and javascript.",
  "main": "dist/main.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./dist && tsc && npm run gen:size_badge",
    "gen:size_badge": "rimraf ./tmp && mkdir -p ./assets && rollup --config && node ./scripts/gen_size_badge_url.mjs |  xargs curl -o ./assets/size.badge.svg",
    "demo": "npx ts-node ./demo/main.ts",
    "perf": "npx ts-node ./demo/perf.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olian04/simply-result.git"
  },
  "bugs": {
    "url": "https://github.com/Olian04/simply-result/issues"
  },
  "homepage": "https://github.com/Olian04/simply-result#readme",
  "keywords": [
    "monad",
    "result",
    "option",
    "typescript",
    "typesafe"
  ],
  "author": "Olian04",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/benchmark": "^2.1.3",
    "@types/node": "^20.7.0",
    "benchmark": "^2.1.4",
    "gzip-size": "^7.0.0",
    "microtime": "^3.1.1",
    "rimraf": "^5.0.4",
    "rollup": "^3.29.3",
    "typescript": "^5.2.2"
  }
}